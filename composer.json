{
	"name": "rkr/php-db-migrations",
	"description": "Not yet powerful library to execute db-migration files",
	"homepage": "https://github.com/rkrx/db-migrations",
	"license": "MIT",
	"type": "library",
	"authors": [{
		"name": "Ron Kirschler",
		"email": "ron.kirschler@gmail.com"
	}],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">= 8.0",
		"ext-pdo": "*",
		"psr/log": "~1"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.0",
		"phpstan/phpstan": ">= 0.1",
		"jetbrains/phpstorm-attributes": "*"
	},
	"suggest": {
		"jetbrains/phpstorm-attributes": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Kir\\DB\\Migrations\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Kir\\DB\\Migrations\\": "tests/"
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse -c phpstan.neon"
	}
}
